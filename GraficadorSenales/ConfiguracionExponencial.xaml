<Page x:Class="GraficadorSenales.ConfiguracionExponencial"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GraficadorSenales"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ConfiguracionExponencial">

    <Grid x:Name="txtAlpha">
        <TextBox x:Name="txtAlpha" HorizontalAlignment="Left" Height="23" Margin="10, 84, 0, 0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="lblAlpha" HorizontalAlignment="Left" Margin="10,49,0,0" TextWrapping="Wrap" Text="Alpha" VerticalAlignment="Top"/>
    </Grid>
</Page>
